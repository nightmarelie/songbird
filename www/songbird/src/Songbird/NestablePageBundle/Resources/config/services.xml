<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="songbird.admin.page" class="Songbird\NestablePageBundle\Admin\PageAdmin">
            <tag name="sonata.admin" manager_type="orm" group="admin" label="Page"/>
            <argument>null</argument>
            <argument>Songbird\NestablePageBundle\Entity\Page</argument>
            <argument>SongbirdNestablePageBundle:PageAdmin</argument>
        </service>
        <service id="songbird.admin.pagemeta" class="Songbird\NestablePageBundle\Admin\PageMetaAdmin">
            <tag name="sonata.admin" manager_type="orm" group="admin" label="Page"/>
            <argument>null</argument>
            <argument>Songbird\NestablePageBundle\Entity\PageMeta</argument>
            <argument>SongbirdNestablePageBundle:PageMetaAdmin</argument>
    </service>
    </services>
</container>
